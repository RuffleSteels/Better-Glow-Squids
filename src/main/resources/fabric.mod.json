{
  "schemaVersion": 1,
  "id": "santapexie_bettergs",
  "version": "${version}",

  "name": "Better Glow Squids",
  "description": "Adds new and better functions to the newly added 'Glow Squids' such as glowing potions! When you splash a regular squid with a glowing potion, it turns into a glow squid!",
  "authors": [
    "Santapexie"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/santapexie_bettergs/gs_icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.santapexie.santapexie_bettergs.BetterGlowSquidsMain"
    ],
    "client": [
      "com.santapexie.santapexie_bettergs.client.BetterGlowSquidsClient"
    ]
  },
  "mixins": [
    "santapexie_bettergs.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
  }
}
